        .errormsg {
          @apply text-red-500 text-xs italic
        }
        .btn {
          @apply py-2 px-4 mr-4 bg-teal-500 text-white font-semibold rounded-lg shadow-md hover:bg-teal-700 focus:outline-none focus:ring-2 focus:ring-teal-400 focus:ring-opacity-75
