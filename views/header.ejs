        .heartlike {
          @apply text-red-500 text-xl mr-4
        }
        .heartunlike {
          @apply text-gray-200 text-xl mr-4
        }
        .field-values {
          @apply text-gray-700 pb-3 leading-tight
        }
        div .datepicker-header {
          @apply h-10 bg-gray-700
        }
        div .datepicker-footer {
          @apply hidden
        }
        div .timepicker-head {
          @apply bg-teal-500
        }
        div .timepicker-button {
          @apply text-teal-700
        }
        .table-cell {
          @apply text-sm text-gray-900 px-6 py-4 whitespace-nowrap
        }
        .table-header {
          @apply text-sm font-medium text-gray-900 px-6 py-4 text-left
        }
